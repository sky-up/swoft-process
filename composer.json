  "keywords": [
    "php",
    "swoole",
    "swoft",
    "process"
  ],
  "description": "Process Component for Swoft",
  "homepage": "https://github.com/sky-up/swoft-process",
  "license": "Apache-2.0",
  "require": {
    "swoft-fork/framework": "~2.1.0"
  },
  "autoload": {
    "psr-4": {
